ces) */}\n      {/* <input type=\"submit\" value=\"click\"></input> */}\n      <br />\n      <button>List My Property!</button>\n    </form>\n    // </div>\n  );\n};\n\nexport default ListProperty;\n","import React from 'react';\nimport './propertyPictures.styles.css';\n\nimport ImageGallery from 'react-image-gallery';\n\nconst PropertyPictures = ({ pics }) => {\n  console.log('PICS: ', pics);\n  return (\n    <div className=\"property-pictures\">\n      <ImageGallery\n        items={pics.map((pic) => ({ original: pic, thumbnail: pic }))}\n      />\n    </div>\n  );\n};\n\nexport default PropertyPictures;\n","import React from 'react';\nimport { useHistory } from 'react-router-dom';\nimport './ownerAuctionInterface.styles.css';\n\nconst OwnerAuctionInterface = (props) => {\n  const history = useHistory();\n  console.log(history);\n  console.log('PROPS.PROPERTY AT OAI: ', props.property);\n  return (\n    <div className=\"owner-auction-interface\">\n      <button\n        onClick={() =>\n          history.push({\n            pathname: `${history.location.pathname}/bids`,\n            // state: props.bids,\n            state: props.property?.auction.bids,\n          })\n        }\n      >\n        See all bids\n      </button>\n\n      <button\n        className=\"button-secondary\"\n        onClick={() =>\n          history.push({\n            pathname: `${history.location.pathname}/edit`,\n            // state: props.auction,\n            //TODO: figure out how to populate existing listing terms on form\n          })\n        }\n      >\n        Change listing terms\n      </button>\n    </div>\n  );\n};\n\nexport default OwnerAuctionInterface;\n","import React from 'react';\n// import { useHistory } from 'react-router-dom';\nimport { withRouter } from 'react-router-dom';\n//I cannot use props from Broswer Router because this is not a page component that can be listed inside of Broswer Router. This component only loads within another page. This is not a page.\n\ncon